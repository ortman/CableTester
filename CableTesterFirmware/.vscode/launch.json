{
  "configurations": [
		{
			"name": "Debug",
			"type": "cortex-debug",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"servertype": "openocd",
			"interface": "swd",
			"executable": "./build/${workspaceFolderBasename}.elf",
			"svdFile": "STM32F103.svd",
			"device": "STM32F103VBT6",
			//"runToEntryPoint": "main",
			"configFiles": [
				"interface/stlink.cfg",
				// ".vscode/stm32f1x-china.cfg"
				"target/stm32f1x.cfg"
			],
			"preLaunchTask": "Build",
			//"showDevDebugOutput": "raw",
			"swoConfig": {
				"enabled": true,
				"cpuFrequency": 72000000,
				"swoFrequency": 2000000,
				"source": "probe",
				"decoders": [
						{ "type": "console", "label": "ITM", "port": 0 }
				]
			}
		}
  ]
}