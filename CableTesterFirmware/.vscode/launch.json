{
  "configurations": [
		{
			"name": "Debug",
			"type": "cortex-debug",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"servertype": "openocd",
			"interface": "swd",
			"executable": "./build/${workspaceFolderBasename}.elf",
			"svdFile": "STM32F103.svd",
			"device": "STM32F103VBT6",
			//"runToEntryPoint": "main",
			"configFiles": [
				".vscode/jlink-stlink.cfg",
				// "interface/jlink.cfg",
				// ".vscode/stm32f1x-china.cfg",
				"target/stm32f1x.cfg"
			],
			"preLaunchTask": "Build",
			"showDevDebugOutput": "raw",
			"rttConfig": {
				"enabled": true,
				"address": "auto",
				"decoders": [
						{ "type": "console", "label": "", "port": 0, "iencoding": "ascii" }
				]
			}
		}
  ]
}